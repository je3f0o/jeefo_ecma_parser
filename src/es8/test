async function async_fn () {
    await async_fn();
    return 1;
}

fn = async function          () {}
fn = async param => {}

fn = () => {}


class A extends B {
    ;

    3.14       () {}
    "str"      () {}
    static     () {}
    [computed] () {}

    // Getters
